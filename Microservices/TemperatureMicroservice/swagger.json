            "type": "integer",
            "format": "int32"
          },
          "StationName": {
            "type": "string",
            "nullable": true
          },
          "Latitude": {
            "type": "number",
            "format": "double"
          },
          "Longitude": {
            "type": "number",
            "format": "double"
          },
          "Timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "AirTemperature": {
            "type": "number",
            "format": "double"
          },
          "RoadTemperature": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "TimeframeDTO": {
        "type": "object",
        "properties": {
          "Seconds": {
            "type": "integer",
            "format": "int32"
          },
          "ReferentTime": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "LocationRadiusDTO": {
        "type": "object",
        "properties": {
          "Latitude": {
            "type": "number",
            "format": "double"
          },
          "Longitude": {
            "type": "number",
            "format": "double"
          },
          "RadiusMeters": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "LocationTimeDTO": {
        "type": "object",
        "properties": {
          "Latitude": {
            "type": "number",
            "format": "double"
          },
          "Longitude": {
            "type": "number",
            "format": "double"
          },
          "RadiusMeters": {
            "type": "number",
            "format": "double"
          },
          "ReferenceTime": {
            "type": "string",
            "format": "date-time"
          },
          "Seconds": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "AverageDTO": {
        "type": "object",
        "properties": {
          "Timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "PerHour": {
            "type": "boolean"
          },
          "LocationInfo": {
            "$ref": "#/components/schemas/LocationRadiusDTO"
          }
        },
        "additionalProperties": false
      }
    }
