      "post": {
        "tags": [
          "Temp"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AverageDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AverageDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AverageDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
