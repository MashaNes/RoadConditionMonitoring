            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LocationRadiusDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LocationRadiusDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/temp_microservice/get-data-location-timeframe": {
      "post": {
        "tags": [
          "Temp"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LocationTimeDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LocationTimeDTO"
